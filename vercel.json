{
    "version": 2,
    "builds": [
      {
        "src": "backend/**",
        "use": "@vercel/php"
      },
      {
        "src": "frontend/package.json",
        "use": "@vercel/react"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "backend/public/$1"
      }
    ]
  }
  